        <activity
            android:name=".Utilites.Conatact"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Intros.intro2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Notifications.Notifications"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name=".Activities.Notifications" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Utilites.StartUp"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.firebase.auth.internal.FederatedSignInActivity"
            android:launchMode="standard"
            tools:replace="android:launchMode" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_icon" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/ic_profile_background" />
    </application>

</manifest>